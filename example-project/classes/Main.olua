require "Entities.Player"
require "EntitySystem"

class Main
	function Main()
		self.x = 1
		self.y = 1
		self.es = new EntitySystem()
		self.es:add( new Player() )
	end

	function update( dt )
		self.es:update()
	end

	function draw()
		self.es:draw()
	end
end